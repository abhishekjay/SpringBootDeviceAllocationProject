package com.deloitte.device.entity;

public class DeviceTable {
	
	private Integer deviceId;
	private String deviceName;
	private String deviceCity;
	public DeviceTable() {
		super();
		// TODO Auto-generated constructor stub
	}
	public DeviceTable(Integer deviceId, String deviceName, String deviceCity) {
		super();
		this.deviceId = deviceId;
		this.deviceName = deviceName;
		this.deviceCity = deviceCity;
	}
	public Integer getDeviceId() {
		return deviceId;
	}
	public void setDeviceId(Integer deviceId) {
		this.deviceId = deviceId;
	}
	public String getDeviceName() {
		return deviceName;
	}
	public void setDeviceName(String deviceName) {
		this.deviceName = deviceName;
	}
	public String getDeviceCity() {
		return deviceCity;
	}
	public void setDeviceCity(String deviceCity) {
		this.deviceCity = deviceCity;
	}
	@Override
	public String toString() {
		return "DeviceTable [deviceId=" + deviceId + ", deviceName=" + deviceName + ", deviceCity=" + deviceCity + "]";
	}
	
	

}
