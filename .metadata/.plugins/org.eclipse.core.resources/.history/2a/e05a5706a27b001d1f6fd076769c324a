import java.util.List;

public class Response {
	
	private DeviceTable device;
	private List<Order> orders;
	private List<Order> orders1;
	
	public Response() {
		super();
		// TODO Auto-generated constructor stub
	}
	
	

	public Response(CustomerTable customer, List<Order> orders, List<Order> orders1) {
		super();
		this.customer = customer;
		this.orders = orders;
		this.orders1 = orders1;
	}



	public CustomerTable getCustomer() {
		return customer;
	}

	public void setCustomer(CustomerTable customer) {
		this.customer = customer;
	}

	public List<Order> getOrders() {
		return orders;
	}

	public void setOrders(List<Order> orders) {
		this.orders = orders;
	}



	public List<Order> getOrders1() {
		return orders1;
	}



	public void setOrders1(List<Order> orders1) {
		this.orders1 = orders1;
	}
	
	

}
package com.deloitte.device.model;

public class Response {

}
